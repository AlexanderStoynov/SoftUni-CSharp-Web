@model int
@{
    ViewBag.Title = $"Nums 1 ... {Model}";
}

<h2>@ViewBag.Title</h2>
<ul>
    @for (int i = 1; i <= Model; i++)
    {
        <li>@i</li>
    }
</ul>

    <a asp-action="Limit" asp-route-num="23">First 23 Numbers</a>
    <br/>
<label for="limitInput">Input Limit</label>
    <input type="number" id="limitInput" value="50"/>
    <button class="btn, btn-primary" onclick="SetLimit()">Get Limited Numbers</button>
<br />
    <a href="https://youtube.com" target="_blank">Youtube</a>

    @section Scripts {
        <script src="~/js/numbers.js"></script>
    }
